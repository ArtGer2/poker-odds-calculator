    <property name="styleSheet">
     <string notr="true">QSpinBox {
    background-color: #f5f5f5; /* Light gray background */
    border: 1px solid #b0b0b0; /* Soft border */
    border-radius: 6px; /* Rounded corners */
    padding: 4px 10px; /* Padding inside the spin box */
    font-family: 'Segoe UI', sans-serif; /* Font family */
    font-size: 14pt; /* Font size */
    color: #333; /* Text color */
}

/* Arrow Buttons Styling */
QSpinBox::up-button, QSpinBox::down-button {
    border: 1px solid #b0b0b0; /* Soft border for buttons */
    border-radius: 4px; /* Rounded corners for buttons */
    background-color: #ffffff; /* White background */
    width: 16px; /* Width of the button */
    height: 16px; /* Height of the button */
}

/* Arrow Buttons Hover State */
QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: #e0e0e0; /* Slightly darker background on hover */
}

/* Arrow Buttons Arrow Styling */
QSpinBox::up-arrow, QSpinBox::down-arrow {
    width: 10px; /* Width of the arrow */
    height: 10px; /* Height of the arrow */
}

/* Control Buttons Background and Arrow Color */
QSpinBox::up-button::default, QSpinBox::down-button::default {
    background-color: #d0d0d0; /* Default background color */
}

QSpinBox::up-button::default:hover, QSpinBox::down-button::default:hover {
    background-color: #c0c0c0; /* Hover background color */
}</string>
